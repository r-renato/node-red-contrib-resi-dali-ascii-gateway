  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.11.12",
    "@types/node-red": "^1.2.0",
    "@types/node-red-node-test-helper": "^0.2.2",
    "modbus-serial": "^8.0.5",
    "mutex": "^1.0.2",
    "queue": "^6.0.2"
